{
  "name": "Rock-chrome",
  "description": "这是一个学习chrome插件编写的项目.",
  "version": "1.0.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "/images/theme-16.png",
      "32": "/images/theme-32.png",
      "48": "/images/theme-48.png",
      "128": "/images/theme-128.png"
    }
  },
  "icons": {
    "16": "/images/theme-16.png",
    "32": "/images/theme-32.png",
    "48": "/images/theme-48.png",
    "128": "/images/theme-128.png"
  },
  "options_page": "html/options.html"
}
